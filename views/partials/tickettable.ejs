<table role="grid">
  <thead>
    <tr>
      <th>Date</th>
      <th>Student</th>
      <th>Expectation</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    <% tickets.forEach(function(ticket) { %>
    <tr>
      <td>
        <a href="/tickets/<%= ticket.id %>"
          ><%= ticket.date.substring(0, 10) %>
        </a>
      </td>
      <td>
        <a href="/tickets/<%= ticket.student_id %>"><%= ticket.student_name %></a>
      </td>
      <td>
        <a href="/tickets/<%= ticket.id %>"><%= ticket.expectation %></a>
      </td>
      <td>
        <a href="/tickets/<%= ticket.id %>"><%= ticket.detail %></a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>